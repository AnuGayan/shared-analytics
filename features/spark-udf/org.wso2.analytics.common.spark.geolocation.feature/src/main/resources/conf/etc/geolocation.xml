<GeoLocation>
    <Implementation class="org.wso2.carbon.analytics.apim.spark.geolocation.impl.LocationResolverRdbms">
        <Property name="dataSource">jdbc/GEO_LOCATION_DATA</Property>
        <Property name="persistInDataBase">true</Property>
        <Property name="ipToLongCacheCount">10000</Property>
    </Implementation>
    <Cache>
        <enabled>true</enabled>
        <IpResolveCacheCount>10000</IpResolveCacheCount>
    </Cache>
</GeoLocation>

